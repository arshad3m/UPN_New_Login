<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.upnorway.listeners.CustomListeners" />
	</listeners>

	<test name="partner test">
		<parameter name="test_url"
			value="https://insider.qa.upnorway.net/" />
		<classes>
			<class name="com.upnorway.testcases.Login_test" />
		</classes>
	</test>

	<test name="insider test">
		<parameter name="test_url"
			value="https://insider.qa.upnorway.net/" />
		<classes>
			<class name="com.upnorway.testcases.Login_test" />
		</classes>
	</test>
	<test name="insider2 test">
		<parameter name="test_url"
			value="https://partner.qa.upnorway.net/" />
		<classes>
			<class name="com.upnorway.testcases.Login_test" />
		</classes>
	</test>

</suite>


